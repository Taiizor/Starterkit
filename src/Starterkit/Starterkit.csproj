<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
        <UseRazorSourceGenerator>false</UseRazorSourceGenerator>
        <AddRazorSupportForMvc>false</AddRazorSupportForMvc>
        <RootNamespace>Starterkit</RootNamespace>
        <TargetFramework>net7.0</TargetFramework>
        <DefaultLanguage>en-GB</DefaultLanguage>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
    </PropertyGroup>

    <PropertyGroup>
        <ApplicationIcon>Resources\Starterkit.ico</ApplicationIcon>
        <Version>1.0.3.4</Version>
        <AssemblyVersion>$(Version)</AssemblyVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Starterkit</Title>
        <PackageId>Taiizor.Starterkit</PackageId>
        <Authors>Taiizor</Authors>
        <Copyright>Copyright Â© $([System.DateTime]::Today.ToString(yyyy)) $(Authors)</Copyright>
        <Summary>Starterkit library for projects using .NET Blazor</Summary>
        <Description>$(Summary)
.NET Blazor Starterkit Library - $(PackageId)
        </Description>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageLicenseExpression></PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageReleaseNotes>Minor changes have been made.
Changes are detailed at https://github.com/Taiizor/Starterkit/releases
        </PackageReleaseNotes>
        <PackageTags>ReaL Taiizor Soferity Vegalya Starterkit C# CSharp VBC VB VisualBasic DotNET .NET NET Starter Kit Library Blazor Razor Asp Asp.NET</PackageTags>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <!--<DocumentationFile>..\$(Title)\bin$(OutputPath)\$(Configuration)\$(TargetFramework)\$(Title).xml</DocumentationFile>-->
        <PackageProjectUrl>https://github.com/Taiizor/Starterkit</PackageProjectUrl>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>git://github.com/Taiizor/Starterkit</RepositoryUrl>
        <PackageDescription>$(Description)</PackageDescription>
        <PackageIcon>Starterkit.png</PackageIcon>
        <Company>$(Authors)</Company>
        <Owners>$(Authors)</Owners>
        <AnalysisLevel>preview</AnalysisLevel>
        <LangVersion>preview</LangVersion>
        <NeutralLanguage>$(DefaultLanguage)</NeutralLanguage>
        <FileVersion>$(Version)</FileVersion>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <Configurations>Debug;Release</Configurations>
        <NoWarn>1587,1591</NoWarn>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.SourceLink.GitHub">
            <Version>1.1.1</Version>
            <PrivateAssets>All</PrivateAssets>
        </PackageReference>
    </ItemGroup>

    <ItemGroup>
        <FrameworkReference Include="Microsoft.AspNetCore.App" />
    </ItemGroup>

    <ItemGroup>
        <None Include="..\..\LICENSE">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <None Include="Resources\Starterkit.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <Content Update="Resources\FactorySettings.json">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Content>
    </ItemGroup>

    <ItemGroup>
        <Folder Include="Properties\" />
    </ItemGroup>

</Project>
